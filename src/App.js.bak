class TeamMembers extends Component {
  constructor() {
    super();
    this.state = {
      team_members: []
    }
  }

  componentDidMount() {
    let dataURL = "http://13.228.34.232/wp-json/wp/v2/team_member?_embed";
        fetch(dataURL)
            .then(res => res.json())
            .then(res => {
                this.setState({
                  team_members: res
                })
            })
  }
}


